<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DataSchema.EmployeeEntity" table="employee" schema="adbuczek">
        <id name="idEmployee">
            <column name="id_employee" sql-type="varchar(50)" length="50"/>
        </id>
        <property name="login">
            <column name="login" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="surname">
            <column name="surname" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>