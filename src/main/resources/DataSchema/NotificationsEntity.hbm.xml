<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DataSchema.NotificationsEntity" table="notifications" schema="adbuczek">
        <id name="idNotification">
            <column name="id_notification" sql-type="varchar(50)" length="50"/>
        </id>
        <property name="idReader">
            <column name="id_reader" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="content">
            <column name="content" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>